import ReplicationLibrary
import Synergex.SynergyDE.Select
import System.Collections

subroutine GenerateEmployeeData

    .include "EMPLOYEE" repository, record="employee"

    record
        ch, i4
        response, a6
    endrecord

proc

	xcall delet("REPLICATOR_DATA:EMPLOYEE.ISM")

	open(ch=0,o:i,"REPLICATOR_DATA:EMPLOYEE.ISM",FDL:"@REPLICATOR_XDL:EMPLOYEE.XDL")

	for employee.emp_id from 1 thru 500000
	begin
		employee.emp_first_name = %randomFirstName
		employee.emp_last_name = %randomLastName
		employee.emp_dept = %randomDepartment
		employee.emp_date_of_birth = %randomDateOfBirth
		employee.emp_hire_date = %randomHireDate(employee.emp_date_of_birth)
		employee.emp_hire_time = 0900
		employee.emp_home_ok = %randomBoolean
		employee.emp_paid =  %randomBoolean + 1
		employee.emp_phone_work = %randomPhone
		employee.emp_phone_home = %randomPhone
		employee.emp_phone_cell = %randomPhone
		employee.emp_email = %randomEmail(employee.emp_first_name,employee.emp_last_name)
		employee.emp_address_street = %randomStreet
		xcall randomCityAndState(employee.emp_address_city,employee.emp_address_state)
		employee.emp_address_zip = %randomZipCode(employee.emp_address_state)
		store(ch,employee)
	end

	close ch
	
    xreturn

endsubroutine

subroutine getSeeds
    required in randomNumber, n
    required out seed1, n
    required out seed2, n
    endparams
    record
        now, a20
    endrecord
proc
    now = %datetime
    seed1 = ^i(now(1:8)) * randomNumber
    seed2 = ^i(now(9:8)) * randomNumber
    xreturn
endsubroutine

