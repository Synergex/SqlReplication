function randomDateOfBirth, d8
    static record
        randomNumber, i4, 89544452
    endrecord
    stack record
        now, a20
        seed1, i4
        seed2, i4
        julianNow, i4
        julianAge18, i4
        julianAge65, i4
        daysRange, i4
        randomDaysOver18, i4
        randomDate, d8
    endrecord
proc
    now=%datetime
    julianNow = %jperiod(^d(now(1:8)))
    julianAge18 = julianNow-(18*365)
    julianAge65 = julianNow-(65*365)
    daysRange = julianAge18 - julianAge65
    getSeeds(randomNumber,seed1,seed2)
    xcall randm(randomNumber,seed1,seed2)
    randomDaysOver18 = %rnd(randomNumber/(32767.0/(daysRange-1)))+1
    randomDate = %ndate(julianAge18-randomDaysOver18)
    freturn randomDate
endfunction

function randomHireDate, d8
    required in aDateOfBirth, d8
    endparams
    static record
        randomNumber, i4, 45211145
    endrecord
    stack record
        now, a20
        seed1, i4
        seed2, i4
        julianDateOfBirth, i4
        julianBirthday18, i4
        julianYesterday, i4
        daysRange, i4
        randomDaysOver18, i4
        randomDate, d8
    endrecord
proc
    ;Random date between the employees 18th birthday and yesterday
    now = %datetime
    julianDateOfBirth = %jperiod(aDateOfBirth)
    julianBirthday18 = julianDateOfBirth+(18*365)
    julianYesterday = %jperiod(^d(now(1:8))) - 1
    daysRange = julianYesterday - julianBirthday18
    getSeeds(randomNumber,seed1,seed2)
    xcall randm(randomNumber,seed1,seed2)
    randomDaysOver18 = %rnd(randomNumber/(32767.0/(daysRange-1)))+1
    randomDate = %ndate(julianBirthday18+randomDaysOver18)
    freturn randomDate
endfunction

